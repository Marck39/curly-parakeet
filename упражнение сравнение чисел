static void Main(string[] args)
        {
          int x = Convert.ToInt32(Console.ReadLine());
            int y = Convert.ToInt32(Console.ReadLine());
            if (x>y)
            {
                Console.WriteLine($"{x}>{y}");
            }
                else if(x<y)
            {
                Console.WriteLine($"{x}<{y}");
            }
            else
            {
                Console.WriteLine($"{x}={y}");
            }
            Console.ReadLine();
        }
